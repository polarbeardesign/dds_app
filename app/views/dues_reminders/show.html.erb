<p id="notice"><%= notice %></p>

<p>
  <b>Member:</b>
  <%= @dues_reminder.member.first_name + " " + @dues_reminder.member.last_name %>
</p>

<p>
  <b>Notice name:</b>
  <%= @dues_reminder.notice_name %>
</p>

<p>
  <b>Sent to:</b>
  <%= @dues_reminder.sent_to %>
</p>

<p>
  <b>Text sent:</b>
  <%= @dues_reminder.text_sent %>
</p>

<p>
  <b>Date sent:</b>
  <%= @dues_reminder.created_at.strftime("%Y-%m-%d %H:%M") %>
</p>


