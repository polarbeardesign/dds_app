<h1>Listing test_answers</h1>

<table>
  <tr>
    <th>Test question</th>
    <th>Answer letter</th>
    <th>Answer</th>
    <th>Correct</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @test_answers.each do |test_answer| %>
  <tr>
    <td><%= test_answer.test_question %></td>
    <td><%= test_answer.answer_letter %></td>
    <td><%= test_answer.answer %></td>
    <td><%= test_answer.correct %></td>
    <td><%= link_to 'Show', test_answer %></td>
    <td><%= link_to 'Edit', edit_test_answer_path(test_answer) %></td>
    <td><%= link_to 'Destroy', test_answer, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Test answer', new_test_answer_path %>
