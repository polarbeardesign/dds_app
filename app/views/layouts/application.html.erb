<!DOCTYPE html>
<html>
<head>
  <title>Devil Dog Squadron :: </title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  <meta name="viewport" content="initial-scale=1, maximum-scale=1">

<!-- Global site tag (gtag.js) - Google Analytics -->
<script async src="https://www.googletagmanager.com/gtag/js?id=G-E2JPP02LBK"></script>
<script>
  window.dataLayer = window.dataLayer || [];
  function gtag(){dataLayer.push(arguments);}
  gtag('js', new Date());

  gtag('config', 'G-E2JPP02LBK');
</script>
</head>
<body>

<div id="container">

  <%= render 'layouts/header' %>
  <%= render 'layouts/nav' %>

<div class="main">

  <% if notice %>
    <p id="notice"><%= notice %></p>
  <% end %>
  
  <article>
    <%= yield %>
  </article>

  <aside>
    <%= yield :sidebar %>
  </aside>


</div>

</div>

<footer>
    <%= render 'layouts/footer' %>
</footer>

</div>

</body>
</html>